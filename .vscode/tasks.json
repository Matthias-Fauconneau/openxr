{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "xwin",
			"args": [
				"run",
				"--target",
				"x86_64-pc-windows-gnu"
			],
			"problemMatcher": [
				"$rustc"
			],
			"label": "xwin",
			"presentation": {
				"clear": true
			}
		},
		{
			"type": "cargo",
			"command": "run",
			"args": ["--release"],
			"problemMatcher": [
				"$rustc"
			],
			"label": "release",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}